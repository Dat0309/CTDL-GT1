        <CommandUIDefinition Location="$UICustomAction_RibbonLocation$">
          <Button Id="$UICustomAction_RibbonLocationId$.$ProjectItemName$Button"
                  Alt="$UICustomAction_RibbonLabelText$"
                  Sequence="100"
