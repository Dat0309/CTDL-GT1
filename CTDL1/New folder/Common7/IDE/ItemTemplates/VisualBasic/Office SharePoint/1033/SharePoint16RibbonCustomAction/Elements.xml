<?xml version="1.0" encoding="utf-8"?>
<Elements xmlns="http://schemas.microsoft.com/sharepoint/">
  <CustomAction Id="$guid2$.$ProjectItemName$"
                RegistrationType="$UICustomAction_RegistrationType$"
                RegistrationId="$UICustomAction_RegistrationId$"
                Location="CommandUI.Ribbon"
                Sequence="10001"
                Title="$UICustomAction_Title$">
    <CommandUIExtension>
      <!-- 
      $loc_custom_action_ui_definition_comment1$
      $loc_custom_action_ui_definition_comment2$
      -->
