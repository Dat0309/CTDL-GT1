<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="$OfficeAppType$">
  <Id>$guid1$</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>$loc_ProviderName$</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="$ProjectItemName$" />
  <Description DefaultValue="$ProjectItemName$"/>
  <Capabilities>
    <Capability Name="Mailbox" />
  </Capabilities>
  <DesktopSettings>
    <SourceLocation DefaultValue="$OfficeAppStartPage$" />
    <RequestedHeight>250</RequestedHeight>
  </DesktopSettings>
  <Permissions>ReadItem</Permissions>
  <Rule xsi:type="RuleCollection" Mode="Or">
$if$ ("$RuleMessage$" == "TRUE")    <Rule xsi:type="ItemIs" ItemType="Message"/>
$endif$$if$ ("$RuleAppointment$" == "TRUE")    <Rule xsi:type="ItemIs" ItemType="Appointment"/>
$endif$  </Rule>
  <DisableEntityHighlighting>false</DisableEntityHighlighting>
</OfficeApp>
