<?xml version="1.0" encoding="UTF-8"?>
<!--Created:cb85b80c-f585-40ff-8bfc-12ff4d0e34a9-->
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="$OfficeAppType$">
  <Id>$guid8$</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>$loc_ProviderName$</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="$projectname$" />
  <Description DefaultValue="$projectname$"/>
  <Capabilities>
$if$ ("$HostExcel$" == "TRUE")    <Capability Name="Workbook" />
$endif$$if$ ("$HostPowerPoint$" == "TRUE")    <Capability Name="Presentation" />
$endif$$if$ ("$HostProject$" == "TRUE")    <Capability Name="Project" />
$endif$$if$ ("$HostWord$" == "TRUE")    <Capability Name="Document" />
$endif$$if$ ("$HostOutlook$" == "TRUE")    <Capability Name="Mailbox" />
$endif$  </Capabilities>
$if$ ("$OfficeAppType$" == "MailApp")  <DesktopSettings>
$else$  <DefaultSettings>
$endif$    <SourceLocation DefaultValue="$OfficeAppStartPage$" />
$if$ ("$OfficeAppType$" == "ContentApp")    <RequestedWidth>400</RequestedWidth>
    <RequestedHeight>400</RequestedHeight>
$endif$$if$ ("$OfficeAppType$" == "MailApp")    <RequestedHeight>250</RequestedHeight>
  </DesktopSettings>
$else$  </DefaultSettings>
$endif$$if$ ("$OfficeAppType$" == "MailApp")  <Permissions>ReadItem</Permissions>
$else$  <Permissions>ReadWriteDocument</Permissions>
$endif$$if$ ("$OfficeAppType$" == "MailApp")  <Rule xsi:type="RuleCollection" Mode="Or">
$endif$$if$ ("$RuleMessage$" == "TRUE")    <Rule xsi:type="ItemIs" ItemType="Message"/>
$endif$$if$ ("$RuleAppointment$" == "TRUE")    <Rule xsi:type="ItemIs" ItemType="Appointment"/>
$endif$$if$ ("$OfficeAppType$" == "MailApp")  </Rule>
  <DisableEntityHighlighting>false</DisableEntityHighlighting>
$endif$</OfficeApp>
