<?xml version="1.0" encoding="utf-8"?>
<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
	<TemplateData>
		<DefaultName>$safeprojectname$</DefaultName>
		<TemplateID>Microsoft.CPP.WinRT.SharedHubApplication.WindowsPhone</TemplateID>
		<TemplateGroupID>WinRT-Native</TemplateGroupID>
		<ProjectType>VC</ProjectType>
		<TargetPlatformName>Windows</TargetPlatformName>
		<RequiredPlatformVersion>6.3.0</RequiredPlatformVersion>
		<CreateInPlace>true</CreateInPlace>
		<CreateNewFolder>true</CreateNewFolder>
		<LocationField>Enabled</LocationField>
		<Hidden>true</Hidden>
	</TemplateData>
	<TemplateContent>
		<Project File="WindowsPhone.Application.WindowsPhone.vcxproj" ReplaceParameters="true">
			<Folder Name="Assets">
				<ProjectItem ReplaceParameters="false" TargetFileName="Logo.scale-240.png">Phone.Logo.png</ProjectItem>
				<ProjectItem ReplaceParameters="false" TargetFileName="SmallLogo.scale-240.png">Phone.SmallLogo.png</ProjectItem>
				<ProjectItem ReplaceParameters="false" TargetFileName="SplashScreen.scale-240.png">Phone.SplashScreen.png</ProjectItem>
				<ProjectItem ReplaceParameters="false" TargetFileName="Square71x71Logo.scale-240.png">Phone.Square71x71Logo.png</ProjectItem>
				<ProjectItem ReplaceParameters="false" TargetFileName="StoreLogo.scale-240.png">Phone.StoreLogo.png</ProjectItem>
				<ProjectItem ReplaceParameters="false" TargetFileName="WideLogo.scale-240.png">Phone.WideLogo.png</ProjectItem>
				<ProjectItem ReplaceParameters="false" TargetFileName="HubBackground.theme-dark.png">WindowsPhone.HubBackground.theme-dark.png</ProjectItem>
				<ProjectItem ReplaceParameters="false" TargetFileName="HubBackground.theme-light.png">WindowsPhone.HubBackground.theme-light.png</ProjectItem>
			</Folder>
			<ProjectItem ReplaceParameters="true" TargetFileName="Package.appxmanifest">Package-PhoneAppx.appxmanifest</ProjectItem>
			<ProjectItem ReplaceParameters="true" TargetFileName="HubPage.xaml">WindowsPhone.HubPage.xaml</ProjectItem>
			<ProjectItem ReplaceParameters="true" TargetFileName="HubPage.xaml.cpp">WindowsPhone.HubPage.xaml.cpp</ProjectItem>
			<ProjectItem ReplaceParameters="true" TargetFileName="HubPage.xaml.h">WindowsPhone.HubPage.xaml.h</ProjectItem>
			<ProjectItem ReplaceParameters="true" TargetFileName="ItemPage.xaml">WindowsPhone.ItemPage.xaml</ProjectItem>
			<ProjectItem ReplaceParameters="true" TargetFileName="ItemPage.xaml.cpp">WindowsPhone.ItemPage.xaml.cpp</ProjectItem>
			<ProjectItem ReplaceParameters="true" TargetFileName="ItemPage.xaml.h">WindowsPhone.ItemPage.xaml.h</ProjectItem>
			<ProjectItem ReplaceParameters="true" TargetFileName="SectionPage.xaml">WindowsPhone.SectionPage.xaml</ProjectItem>
			<ProjectItem ReplaceParameters="true" TargetFileName="SectionPage.xaml.cpp">WindowsPhone.SectionPage.xaml.cpp</ProjectItem>
			<ProjectItem ReplaceParameters="true" TargetFileName="SectionPage.xaml.h">WindowsPhone.SectionPage.xaml.h</ProjectItem>
			<ProjectItem ReplaceParameters="true" TargetFileName="$ext_projectname$.WindowsPhone.vcxproj.filters">WindowsPhone.Application.WindowsPhone.vcxproj.filters</ProjectItem>
		</Project>
	</TemplateContent>
	<WizardExtension>
		<Assembly>Microsoft.VisualStudio.WinRT.TemplateWizards, Version=12.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a</Assembly>
		<FullClassName>Microsoft.VisualStudio.WinRT.TemplateWizards.PhoneAppxWizard</FullClassName>
	</WizardExtension>
</VSTemplate>
