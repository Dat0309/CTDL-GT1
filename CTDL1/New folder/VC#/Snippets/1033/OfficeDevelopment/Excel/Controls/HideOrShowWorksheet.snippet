<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Hide or Show a Worksheet</Title>
      <Author>Microsoft Corporation</Author>
      <Description>Hides or shows an Excel worksheet.</Description>
      <HelpUrl></HelpUrl>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Keywords>
        <Keyword></Keyword>
      </Keywords>
      <Shortcut></Shortcut>
    </Header>
    <Snippet>
      <References>
        <Reference>
          <Assembly>Microsoft.Office.Interop.Excel.dll</Assembly>
          <Url></Url>
        </Reference>
      </References>
      <Imports>
        <Import>
          <Namespace>Excel = Microsoft.Office.Interop.Excel</Namespace>
        </Import>
      </Imports>
      <Declarations>
        <Literal Editable="true">
          <ID>visibility</ID>
          <Type>Excel.XlSheetVisibility</Type>
          <ToolTip>Replace with the visibility level for the worksheet. Choose xlSheetVisible, xlSheetHidden, or xlSheetVeryHidden.</ToolTip>
          <Default>xlSheetHidden</Default>
          <Function></Function>
        </Literal>
        <Object Editable="true">
          <ID>worksheet</ID>
          <Type>Microsoft.Office.Interop.Excel.Worksheet</Type>
          <ToolTip>Replace with the worksheet to show or hide.</ToolTip>
          <Default>worksheet</Default>
          <Function></Function>
        </Object>
      </Declarations>
      <Code Language="CSharp" Kind="" Delimiter="$"><![CDATA[
$worksheet$.Visible = Excel.XlSheetVisibility.$visibility$;
			]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>