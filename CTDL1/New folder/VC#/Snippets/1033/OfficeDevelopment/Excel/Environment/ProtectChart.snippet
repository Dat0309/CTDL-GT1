<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Protect a Chart</Title>
      <Author>Microsoft Corporation</Author>
      <Description>Protects an Excel chart object.</Description>
      <HelpUrl></HelpUrl>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Keywords>
        <Keyword></Keyword>
      </Keywords>
      <Shortcut></Shortcut>
    </Header>
    <Snippet>
      <References>
        <Reference>
          <Assembly>Microsoft.Office.Interop.Excel.dll</Assembly>
          <Url></Url>
        </Reference>
      </References>
      <Imports>
        <Import>
          <Namespace>Excel = Microsoft.Office.Interop.Excel</Namespace>
        </Import>
      </Imports>
      <Declarations>
        <Literal Editable="true">
          <ID>protectDrawingObjects</ID>
          <Type>System.Bool</Type>
          <ToolTip>Replace with a Boolean value. True to protect shapes in the worksheet.</ToolTip>
          <Default>true</Default>
          <Function></Function>
        </Literal>
        <Literal Editable="true">
          <ID>protectContents</ID>
          <Type>System.Bool</Type>
          <ToolTip>Replace with a Boolean value. True protects all locked cells in the worksheet.</ToolTip>
          <Default>true</Default>
          <Function></Function>
        </Literal>
        <Literal Editable="true">
          <ID>userInterfaceOnly</ID>
          <Type>System.Bool</Type>
          <ToolTip>Replace with a Boolean value. True to allow code behind the document to alter its contents.</ToolTip>
          <Default>true</Default>
          <Function></Function>
        </Literal>
        <Literal Editable="true">
          <ID>scenarios</ID>
          <Type>System.Bool</Type>
          <ToolTip>Replace with True to protect scenarios.  This argument is valid only for worksheets. The default is True.</ToolTip>
          <Default>true</Default>
          <Function></Function>
        </Literal>
        <Object Editable="true">
          <ID>chart</ID>
          <Type>Microsoft.Office.Interop.Excel.Chart</Type>
          <ToolTip>Replace with the chart to protect.</ToolTip>
          <Default>chart</Default>
          <Function></Function>
        </Object>
        <Object Editable="true">
          <ID>password</ID>
          <Type>System.String</Type>
          <ToolTip>Replace with the password to protect the worksheet with.</ToolTip>
          <Default>"Password"</Default>
          <Function></Function>
        </Object>
      </Declarations>
      <Code Language="CSharp" Kind="" Delimiter="$"><![CDATA[
$chart$.Protect($password$, $protectDrawingObjects$, $protectContents$, $scenarios$, $userInterfaceOnly$);
			]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>