<?xml version="1.0" encoding="utf-8"?>
<widget version="1.0" spec="1.0" id="ms-blend://Microsoft.VS.Behaviors.TimerBehavior" name="Microsoft.VS.Behaviors.TimerBehavior" xmlns:blend="http://schemas.microsoft.com/winjs/Blend/2011" xmlns="http://openajax.org/metadata">
	<userAgent platform="WindowsWeb" osVersion="8.0"/>
	<userAgent platform="WindowsWeb" osVersion="8.1"/>
	<userAgent platform="WindowsWebPhone" osVersion="8.1"/>
	<locale messages="..\all-all.xml" />
	<locale lang="en-us" messages="..\en-us.xml" />
	<locale lang="de-de" messages="..\de-de.xml" />
	<locale lang="es-es" messages="..\es-es.xml" />
	<locale lang="fr-fr" messages="..\fr-fr.xml" />
	<locale lang="it-it" messages="..\it-it.xml" />
	<locale lang="ja" messages="..\ja.xml" />
	<locale lang="ko" messages="..\ko.xml" />
	<locale lang="ru-ru" messages="..\ru-ru.xml" />
	<locale lang="zh-cn" messages="..\zh-cn.xml" />
	<locale lang="zh-tw" messages="..\zh-tw.xml" />
	<author name="Microsoft Corporation" />
	<title type="text/plain" locid="Microsoft.VS.Behaviors.TimerBehavior.Title"><![CDATA[TimerBehavior]]></title>
	<description type="text/plain" locid="Microsoft.VS.Behaviors.TimerBehavior.Description"><![CDATA[Invokes an action when timer ticks]]></description>
	<icons>
		<icon src="Microsoft.VS.Behavior.16x16.png" width="16" height="16" />
		<icon src="Microsoft.VS.Behavior.12x12.png" width="12" height="12" />
	</icons>
	<properties>
		<property name="type" datatype="String" required="true">
			<description locid="Microsoft.VS.Behaviors.TimerBehavior.type" type="text/plain">The type of the behavior.</description>
		</property>
		<property name="millisecondsPerTick" datatype="Number" required="false" isInteger="true" defaultValue="1000" minimum="0">
			<description locid="Microsoft.VS.Behaviors.TimerBehavior.millisecondsPerTick" type="text/plain">Specifies the time between ticks.</description>
		</property>
		<property name="totalTicks" datatype="Number" required="false" isInteger="true" defaultValue="10" minimum="0">
			<description locid="Microsoft.VS.Behaviors.TimerBehavior.totalTicks" type="text/plain">Specifies the total number of ticks.</description>
		</property>
		<property name="triggeredActions" datatype="ActionCollection" required="false">
			<description locid="Microsoft.VS.Behaviors.TimerBehavior.triggeredActions" type="text/plain">Specifies the collection of actions that will execute when the TimerBehavior is executed.</description>
		</property>
	</properties>
	<requires>
		<require type="javascript" src="..\..\Libraries\HTML\Interactivity.1.1.js" target="js/Interactivity.1.1.js"/>
	</requires>
</widget>